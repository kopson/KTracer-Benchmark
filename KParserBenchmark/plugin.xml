<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="kparserbenchmark.intro.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="kparserbenchmark.intro.Perspective"
            icon="icons/alt_window_16.gif"
            id="KParserBenchmark.perspective"
            name="RCP Perspective">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="KParserBenchmark.application"
            name="KTracer">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            id="kparserbenchmark.intro">
      </intro>
      <introProductBinding
            introId="kparserbenchmark.intro"
            productId="KParserBenchmark.product">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            introId="kparserbenchmark.intro"
            content="introContent.xml"
            id="KParserBenchmark.configId">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx">
            </implementation>
         </presentation>
      </config>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="kparserbenchmark.projectexplorer.ProjectExplorer"
            id="KParserBenchmark.projectExplorer"
            name="Project"
            restorable="true">
      </view>
      <view
            class="kparserbenchmark.projectexplorer.FileBrowser"
            id="kparserbenchmark.projectexplorer.filebrowser"
            name="File Browser"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="kparserbenchmark.commands.ProjectWizardHandler"
            id="KParserBenchmark.newProjectCommand"
            name="New Project">
      </command>
      <command
            defaultHandler="org.eclipse.ui.internal.handlers.IntroHandler"
            id="KParserBenchmark.welcomeCommand"
            name="Welcome">
      </command>
      <command
            defaultHandler="org.eclipse.ui.internal.handlers.QuitHandler"
            id="KParserBenchmark.exitCommand"
            name="Exit">
      </command>
      <command
            defaultHandler="kparserbenchmark.commands.SetWorkspaceHandler"
            id="KParserBenchmark.SetWorkspaceCommand"
            name="SetWorkspace">
      </command>
      <command
            defaultHandler="org.eclipse.ui.internal.about.AboutHandler"
            id="KParserBenchmark.aboutCommand"
            name="About">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="KParserBenchmark.fileMenu"
               label="File">
            <command
                  commandId="KParserBenchmark.newProjectCommand"
                  label="New Project"
                  style="push">
            </command>
            <command
                  commandId="KParserBenchmark.exitCommand"
                  label="Exit"
                  style="push"
                  tooltip="Exit application">
            </command>
         </menu>
         <menu
               id="KParserBenchmark.projectMenu"
               label="Project">
            <command
                  commandId="KParserBenchmark.SetWorkspaceCommand"
                  label="Set workspace"
                  style="push">
            </command>
         </menu>
         <menu
               id="KParserBenchmark.helpMenu"
               label="Help">
            <command
                  commandId="KParserBenchmark.welcomeCommand"
                  label="Welcome"
                  style="push">
            </command>
            <command
                  commandId="KParserBenchmark.aboutCommand"
                  label="About"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>

</plugin>
